{
  "name": "",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "node dev/server.js & npm run watch-js & npm run watch-css & npm run watch-lint",
    "js": "browserify src/js/app.js -o public/js/app.js -t [ babelify --presets [ es2015 ] ]",
    "watch-js": "watchify src/js/app.js -o public/js/app.js -t [ babelify --presets [ es2015 ] ]",
    "css": "node-sass src/css/app.scss > public/css/app.css",
    "watch-css": "node-sass --watch src/css/app.scss public/css/app.css",
    "lint": "eslint src/js",
    "watch-lint": "chokidar src/js/**/*.js -c 'npm run lint'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-preset-es2015": "6.9.0",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "chokidar-cli": "1.2.0",
    "connect": "3.4.1",
    "connect-livereload": "0.5.4",
    "eslint": "3.0.1",
    "node-sass": "3.8.0",
    "serve-static": "1.11.1",
    "vue": "1.0.26",
    "watchify": "3.7.0",
    "webpack": "1.13.1"
  }
}
